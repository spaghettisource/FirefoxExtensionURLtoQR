{

    "description": "Turns the currently opened URL to QR code",
    "manifest_version": 2,
    "name": "url-to-qr",
    "permissions": [
      "find",
      "tabs"
    ],
    "version": "1.0",
    "homepage_url": "http://www.gerasimtrifonov.com",
    "icons": {
      "48": "icons/index.gif"
    },
  
  
    "browser_action": {
      "default_icon": "icons/index.gif",
      "default_title": "QR code",
    "default_popup": "popup/popup.html"
    }
  
  }
  
